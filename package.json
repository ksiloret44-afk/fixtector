  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "db:generate": "prisma generate --schema=prisma/schema-main.prisma && prisma generate --schema=prisma/schema-company.prisma",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:init": "tsx scripts/init-db.ts",
    "setup": "npm install && npm run db:generate && npm run db:push && npm run db:init",
    "postinstall": "npm run db:generate"
  },
